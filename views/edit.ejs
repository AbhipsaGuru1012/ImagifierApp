<%- include("./partials/header") %>


<div class="container">
	<h1>
		Edit image
	</h1>
	<form action="/<%=image._id%>?_method=PUT" method="POST">
		<div class="form-group">
			<input type="text" value="<%=image.title%>" name="image[title]">
		</div>
		<div class="form-group">
			<input type="text" value="<%=image.desc%>" name="image[desc]">
		</div>
		<div class="form-group">
			<input type="text" value="<%=image.url%>" name="image[url]">
		</div>
		<div class="form-group">
			<button class="btn btn-lg btn-success">
				Submit
			</button>
		</div>
	</form>
</div>





<%- include("./partials/footer") %>